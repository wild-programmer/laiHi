
.contente {
  background-color: white;
  border-bottom: 2px solid  #F3F3F3 ;
}
.contente .contentel {
  float: left;
  width: 200rpx;
  height: 140rpx;
  background-color: pink;
  margin: 20rpx 10rpx  20rpx 20rpx;
}
.contente .contentel image{
  width: 100%;
  height: 100%;
}
.contente .contenter .ul {
  padding-top: 25rpx;
  line-height: 50rpx;
}
.contente .contenter .ul .li:first-child {
  font-size: 28rpx;
  line-height: 37rpx;
  color: #333333;
  width: 510rpx;
}
.contente .contenter .ul .ulC {
  margin-top: 10rpx;
  font-size: 20rpx;
  height: 28rpx;
  line-height: 28rpx;
  color: #9b9b9b;
}
.contente .contenter .ul .ulC .span:last-child {
  margin-left: 30rpx;
}
.toudicon {
  position: relative;
}
.toudicon::before {
  content: '';
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  background-color: #333;
  opacity: .7;
}
.TouDiWin {
  display: none;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  background-color: white;
  text-align: center;
  box-sizing: border-box;
  padding-top: 25rpx;
}
.TouDiWin .p {
  font-size: 25rpx;
  height: 25rpx;
  line-height: 25rpx;
}
.TouDiWin .p:last-child {
  margin-top: 15rpx;
}
.block{
    display: block;
}
.block::before{
    display: block;
}
