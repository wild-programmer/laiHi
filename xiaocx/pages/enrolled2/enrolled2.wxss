}
.contentB .contentBr{
  height: 200rpx;
  line-height: 200rpx;
  margin-left: 565rpx;
  text-align: center;
  background-color: #97d78d;
  color: white;
  font-size: 37rpx;
}

.contentB .i{
  content: '';
  display: block;
  position: absolute;
  left: -15rpx;
  top: 2rpx;
  width: 10px;
  height: 10px;
  background-color: #ededed;
  border-radius: 50%;
}