

.contentT {
  height: 130rpx;
  width: 100%;
  margin-top: 15rpx;
  background-color: white;
}

.contentT .contentTl {
  width: 145rpx;
  height: 100rpx;
  float: left;
  margin-top: 15rpx;
  margin-left: 20rpx;
  background-color: pink;
}
.contentTl image{
  /*width:80px;
    height:55px;*/
    width: 100%;
    height: 100%;
}
.contentT .contentTr {
  margin-left: 174rpx;
  font-size: 22rpx;
  padding-top: 25rpx;
}
.contentT .contentTr .contentTrT {
  padding-right: 10rpx;
}
.contentT .contentTr .contentTrB {
  height: 20rpx;
  line-height: 20rpx;
  margin-top: 10rpx;
  color: #9b9b9b;
}
.contentTB {
  padding-top: 21rpx;
}
.contentTB .contentTBC {
  width: 630rpx;
  margin: 19rpx  60rpx 0 60rpx;
  background-color: white;
  border-radius: 10rpx;
  padding-top: 30rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  border: 1px solid #DB1F57;
  position: relative;
}
.contentTB .contentTBC .p {
  font-size: 26rpx;
  height: 26rpx;
  line-height: 26rpx;
}
.contentTB .contentTBC .span {
  display: block;
  font-size: 20rpx;
  height: 20rpx;
  line-height: 20rpx;
  margin-top: 17rpx;
  color: #DB1F57;
  margin-bottom: 30rpx;
}
.contentTB .contentTBC .li {
  position: relative;
  text-indent: 15rpx;
  padding-left: 10rpx;
  padding-right: 50rpx;
  letter-spacing: .5px;
  margin-bottom: 30rpx;
  color: #9b9b9b;
  font-size: 20rpx;
}
.contentTB .contentTBC .li.active {
  display: none;
}
.contentTB .contentTBC .li:before {
  display: block;
  content: '*';
  color: #DB1F57;
  width: 10rpx;
  height: 10rpx;
  position: absolute;
  left: -15rpx;
  top: 0;
}
.contentTB .contbefore:before {
  display: block;
  content: '';
  width: 43rpx;
  height: 43rpx;
  background: url('../img/select.svg');
  background-size: 100%;
  position: absolute;
  right: -4rpx;
  top: -4rpx;
}
.contentTf {
  position: absolute;
  bottom: 25rpx;
  left: 50%;
  transform: translateX(-50%);
  text-align: center;
}
.contentTf button {
  width: 630rpx;
  height: 80rpx;
  background-color: #DB1F57;
  color: white;
  margin-top: 15rpx;
}
