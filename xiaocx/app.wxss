
.clearfix:before,
.clearfix:after {
    content: "";
    display: table;
}

.clearfix:after {
    clear: both;
}


.one_txt_cut {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}



.txt_cut {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
view{
    font-family: '微软雅黑',Microsoft YaHei;
